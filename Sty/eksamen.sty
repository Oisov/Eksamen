      \definecolor{maincolorLight}{HTML}{c48fcd}%
      \definecolor{maincolorMedium}{HTML}{a757b2}%
      \definecolor{maincolorDark}{HTML}{9637a0}%
      \newcommand{\maincolor}{F}\def\Label{1P + 2P}%
    \def\Labeltre{\small Matematikk 1P og 2P Forkurs Lærerutdanningene  }}%
    {U}{\newcommand{\maincolor}{U}\def\Label{}%
    \def\Labeltre{MAT0010 Matematikk}}%
    {P}{\newcommand{\maincolor}{P}\def\Label{$#2$P}%
      \def\Labeltre{\ifthenelse{\equal{#2}{2}}{MAT1015 Matematikk 2P}{%
    \ifthenelse{\equal{#2}{1}}{MAT1011 Matematikk 1P}{} }}}%
    {T}{\newcommand{\maincolor}{T}\def\Label{$#2$T}%
      \def\Labeltre{\ifthenelse{\equal{#2}{2}}{MAT1017 Matematikk 2T}{%
    \ifthenelse{\equal{#2}{1}}{MAT1013 Matematikk 1T}{} }}}%
    {R}{\newcommand{\maincolor}{R}\def\Label{R#2}%
      \def\Labeltre{\ifthenelse{\equal{#2}{2}}{REA3024 Matematikk R2}{%
    \ifthenelse{\equal{#2}{1}}{REA1022 Matematikk R1}{} }}}%
    {S}{\newcommand{\maincolor}{S}\def\Label{S#2}%
      \def\Labeltre{\ifthenelse{\equal{#2}{2}}{REA2028 Matematikk S2}{%
    \ifthenelse{\equal{#2}{1}}{REA2026 Matematikk S1}{} }}}%
  }{}%
  \IfStrEqCase{#3}{%
    {H}{\def\Labelto{ Høst } \def\Labelfire{Høsten \Year} \refstepcounter{host}}%
    {V}{\def\Labelto{ Vår } \def\Labelfire{Våren \Year}}%
  }[\def\Labelto{ #3 } \def\Labelfire{Høsten \Year}]%

  \IfStrEqCase{#6}{%
    {O}{\def\ExamOrProveText{\eksamenOrProve}}%
    {U}{\def\ExamOrProveText{Utsatt~\eksamenOrProve}}%
  }[\def\ExamOrProveText{\eksamenOrProve}]

